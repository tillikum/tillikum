{
    "name": "tillikum/tillikum",
    "description": "The Tillikum Project",
    "homepage": "http://tillikum.org/",
    "license": "GPL-2.0",
    "autoload": {
        "psr-0": {
            "Tillikum\\": "library/",
            "Tillikum_": "library/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gwis/php-valueobjects"
        },
        {
            "type": "vcs",
            "url": "https://github.com/guilhermeblanco/zendframework1-doctrine2"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tillikum/zf2-cas-authentication-adapter"
        }
    ],
    "require": {
        "beberlei/DoctrineExtensions": "dev-master",
        "doctrine/orm": "~2.3.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "guilhermeblanco/zendframework1-doctrine2": "dev-master",
        "gwis/php-valueobjects": "dev-master",
        "php": ">=5.3.3",
        "tillikum/zf2-cas-authentication-adapter": "dev-master",
        "zendframework/zendframework1": "~1.12.0",
        "zendframework/zendframework": "~2.2.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "dev-master",
        "phing/phing": "~2.5.0"
    },
    "suggest": {
        "ext-apc": "A free and open opcode cache for PHP. Very important for performance, and useful for caching.",
        "ext-memcached": "PHP extension for interfacing with memcached via libmemcached library."
    }
}
